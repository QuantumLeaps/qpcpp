Q_REQUIRE_INCRIT(300, obj_kind &lt; Q_DIM(rxPriv_.currObj));

QS_MEM_SYS();

rxPriv_.currObj[obj_kind] = obj_ptr;

QS_MEM_APP();
QS_CRIT_EXIT();</code>
