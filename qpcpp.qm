    else { // blocking did NOT time out
        // the thread must NOT be waiting on this semaphore
        Q_ASSERT_INCRIT(250, !m_waitSet.hasElement(p));
    }
    curr-&gt;m_temp.obj = nullptr; // clear blocking obj.
}
QF_MEM_APP();
QF_CRIT_EXIT();

return taken;</code>
