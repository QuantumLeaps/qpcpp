Q_REQUIRE_INCRIT(200, QEvt::verify_(e));

std::uint8_t const pcopy = static_cast&lt;std::uint8_t&gt;(~m_prio_dis);
Q_INVARIANT_INCRIT(202, m_prio == pcopy);
