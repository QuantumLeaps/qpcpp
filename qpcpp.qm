QP::QS::rxPriv_.chksum = 0U;

beginRec_(static_cast&lt;std::uint_fast8_t&gt;(QS_OBJ_DICT));
    QS_OBJ_PRE_(&amp;rxPriv_);
    QS_STR_PRE_(&quot;QS_RX&quot;);
