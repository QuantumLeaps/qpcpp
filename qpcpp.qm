  <class name="QFreeBlock">
   <!--${QF::QFreeBlock::m_next}-->
   <attribute name="m_next" type="QFreeBlock *" visibility="0x02" properties="0x00"/>
   <!--${QF::QFreeBlock::m_next_dis}-->
