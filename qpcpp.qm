        QS_SIG_PRE_(e-&gt;sig);  // the signal of this event
        QS_OBJ_PRE_(this);    // this active object
        QS_2U8_PRE_(e-&gt;getPoolNum_(), e-&gt;refCtr_); // poolNum &amp; refCtr
        QS_EQC_PRE_(nFree);   // # free entries
    QS_END_PRE_()
