   <!--${QXK-extern-C::QXK_Attr::lockCeil}-->
   <attribute name="lockCeil" type="std::uint_fast8_t volatile" visibility="0x00" properties="0x00"/>
   <!--${QXK-extern-C::QXK_Attr::lockHolder}-->
